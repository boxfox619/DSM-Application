<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:sl="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.administrator.myapplication.webFragment"
    android:id="@+id/rootLayouts2">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.design.widget.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/AppTheme.AppBarOverlay"
            android:id="@+id/view">
            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                app:layout_collapseMode="pin"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
                app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />
        </android.support.design.widget.AppBarLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/scrollView6"
            android:fillViewport="false">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.ssomai.android.scalablelayout.ScalableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/test"
                    sl:scale_base_width="100"
                    sl:scale_base_height="74">
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="연장학습 신청"
                    android:id="@+id/checkinbtn"
                    android:background="#3366FF"
                    android:textStyle="bold"
                    android:textColor="#ffffff"
                    android:paddingTop="3dp"
                    android:paddingBottom="3dp"
                    android:paddingLeft="5dp"
                    android:singleLine="true"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_marginTop="7dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    sl:scale_left="2"
                    sl:scale_top="2"
                    sl:scale_width="96"
                    sl:scale_height="11"
                    sl:scale_textsize="6" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="내정보 &amp; 수정"
                    android:id="@+id/textView11"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center_vertical|center_horizontal"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:background="#3366FF"
                    android:textColor="#ffffff"
                    android:textStyle="bold"
                    sl:scale_left="2"
                    sl:scale_top="15"
                    sl:scale_width="96"
                    sl:scale_height="11"
                    sl:scale_textsize="6"  />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="이룸!"
                        android:id="@+id/IDTEXT"
                        android:background="@color/colorPrimary"
                        android:textStyle="bold"
                        android:textColor="#ffffff"
                        android:paddingTop="3dp"
                        android:paddingBottom="3dp"
                        android:paddingLeft="5dp"
                        android:singleLine="true"
                        android:gravity="center_vertical|center_horizontal"
                        sl:scale_left="3"
                        sl:scale_top="27"
                        sl:scale_width="94"
                        sl:scale_height="10"
                        sl:scale_textsize="5"  />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="비밀번호 변경"
                        android:id="@+id/Chagne"
                        android:background="@color/colorPrimary"
                        android:layout_marginTop="5dp"
                        android:textColor="#ffffff"
                        android:textStyle="bold"
                        sl:scale_left="3"
                        sl:scale_top="38"
                        sl:scale_width="94"
                        sl:scale_height="10"
                        sl:scale_textsize="5"  />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:text="로그아웃"
                        android:id="@+id/button"
                        android:background="@color/colorPrimary"
                        android:layout_marginTop="5dp"
                        android:textColor="#ffffff"
                        android:textStyle="bold"
                        android:layout_gravity="center_horizontal"
                        sl:scale_left="3"
                        sl:scale_top="49"
                        sl:scale_width="94"
                        sl:scale_height="10"
                        sl:scale_textsize="5"  />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="내 스크랩 정보"
                        android:id="@+id/textView17"
                        android:layout_gravity="center_horizontal"
                        android:gravity="center_vertical|center_horizontal"
                        android:paddingTop="8dp"
                        android:paddingBottom="8dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_below="@+id/layouts"
                        android:background="#E91E63"
                        android:textColor="#ffffff"
                        android:textStyle="bold"
                        sl:scale_left="2"
                        sl:scale_top="63"
                        sl:scale_width="96"
                        sl:scale_height="11"
                        sl:scale_textsize="6" />
                </com.ssomai.android.scalablelayout.ScalableLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/layouts2"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingTop="3dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignRight="@+id/textView17"
                    android:layout_alignEnd="@+id/textView17"
                    android:gravity="center_horizontal">

                </LinearLayout>

            </LinearLayout>
        </ScrollView>


    </LinearLayout>

</RelativeLayout>